<doc domain="036.vn" index="domain-vn-CC-MAIN-2020-16-000" url="http://036.vn/148/11897/rba-giam-lai-suat-xuong-0-75.html" warc-file="crawl-data/CC-MAIN-2020-16/segments/1585370519111.47/warc/CC-MAIN-20200404011558-20200404041558-00210.warc.gz" offset="155203" length="25237" response="200" mime-type="text/html">
